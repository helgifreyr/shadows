 RowBox[{
  RowBox[{"shadow", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"completeShadow1", ",", "completeShadow2"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"obsShadow", "=", 
