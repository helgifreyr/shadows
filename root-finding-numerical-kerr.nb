       RowBox[{"Length", "[", "shadow", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{"Do", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"xObs", "=", 
     RowBox[{
      RowBox[{"shadow", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "1"}], "]"}], "]"}], "/", 
      RowBox[{"Sqrt", "[", 
       RowBox[{
        RowBox[{"\[Phi]\[Phi]", "/.", 
         RowBox[{"r", "\[Rule]", 
          RowBox[{"ics", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}], "/.", 
        RowBox[{"\[Theta]", "\[Rule]", 
         RowBox[{"ics", "[", 
          RowBox[{"[", "3", "]"}], "]"}]}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"yObs", "=", 
     RowBox[{
      RowBox[{"shadow", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "2"}], "]"}], "]"}], "/", 
      RowBox[{"Sqrt", "[", 
       RowBox[{
        RowBox[{"\[Theta]\[Theta]", "/.", 
         RowBox[{"r", "\[Rule]", 
          RowBox[{"ics", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}]}], "/.", 
